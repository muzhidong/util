{
  "name": "js-util",
  "version": "1.0.0",
  "description": "此库用于积累开发过程中有用的JS工具函数",
  "scripts": {
    "minify": "uglifyjs ./src/$FILE_DIR/$FILE_NAME.js -m -c -o ./build/$FILE_NAME.min.js",
    "minify-help": "uglifyjs -h",
    "build": "cross-env FILES=$FILES webpack",
    "docs": "rm -rf docs && jsdoc -c ./jsdoc.config.json",
    "jsdoc-help": "jsdoc -h",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "better-docs": "^2.3.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "docdash": "^1.2.0",
    "jest": "^26.6.3",
    "jsdoc": "~3.6.6",
    "puppeteer": "^8.0.0",
    "uglify-js": "^3.12.1",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1"
  }
}
